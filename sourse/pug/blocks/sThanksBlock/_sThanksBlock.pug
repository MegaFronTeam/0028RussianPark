mixin sThanksBlock(data)
	// start sThanksBlock
	+b.SECTION.sThanksBlock.section#sThanksBlock&attributes(attributes)
		.container
			+e.slider.swiper.defaultSwiper-js
				+b.section-title(data-aos="fade-up" data-aos-delay="0")
					.row
						.col
							h2 Благодарности
						.col-auto.d-none.d-md-block
							+e.arrow-wrap
								+arrow
				.swiper-wrapper
					mixin slide(i)
						+e.slide.swiper-slide(data-aos="fade-up" data-aos-delay='0')
							+e.img-wrap(href=`img/sThanksBlock-img-lg-${i}.png` data-fancybox="gallery")
								.img-wrap-center
									+img(`img/sThanksBlock-img-lg-${i}.png`, 'bg')
					- for(let i = 1; i <=8; i++)
						+slide(i)
	// end sThanksBlock