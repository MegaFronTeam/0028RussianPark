mixin sOurProjects(data)
	// start sOurProjects
	+b.SECTION.sOurProjects.section#sOurProjects&attributes(attributes)
		.container
			+b.section-title(data-aos="fade-up" data-aos-delay="0")
				+e.row.row
					.col
						h2 Наши проекты
					.col-auto
						+e.all-content(href='#')
							| Все проекты
							+e.icon-wrap
								+icon('chevron-right')
			+e.swiper--js.swiper
				+e.row.row.swiper-wrapper
					+e.col.col-md-6.swiper-slide(data-aos="fade-up" data-aos-delay="0")
						+card({
							img: 'project-item-bg-1.jpg',
							title: 'Братеевская Пойма',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						}).card-item--horizontal
					+e.col.col-md-3.swiper-slide(data-aos="fade-up" data-aos-delay="50")
						+card({
							img: 'project-item-bg-2.jpg',
							title: 'Ливны',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						})
					+e.col.col-md-3.swiper-slide(data-aos="fade-up" data-aos-delay="100")
						+card({
							img: 'project-item-bg-3.jpg',
							title: 'Братеевская Пойма',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						})
					+e.col.col-md-3.swiper-slide(data-aos="fade-up" data-aos-delay="150")
						+card({
							img: 'project-item-bg-4.jpg',
							title: 'Москва ул. Декабристов',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						})
					+e.col.col-md-3.swiper-slide(data-aos="fade-up" data-aos-delay="200")
						+card({
							img: 'project-item-bg-5.jpg',
							title: 'Темрюк',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						})
					+e.col.col-md-6.swiper-slide(data-aos="fade-up" data-aos-delay="250")
						+card({
							img: 'project-item-bg-6.jpg',
							title: 'Скитские Пруды',
							text: 'Не&nbsp;все постройки отличает ажурность и&nbsp;миниатюрность&nbsp;&mdash; эту входную группу можно..'
						}).card-item--horizontal
	// end sOurProjects